cont_par = 0
cont_impar = 0
cont_posit = 0
cont_negat = 0

i = 1
while(i <= 5):
    n = int(input())
    if n % 2 == 0:
        cont_par += 1
    else:
        cont_impar += 1
    if n > 0:
        cont_posit += 1
    elif n < 0:
        cont_negat += 1
    i = i + 1

print(cont_par,"valor(es) par(es)")
print(cont_impar,"valor(es) impar(es)")
print(cont_posit,"valor(es) positivo(s)")
print(cont_negat,"valor(es) negativo(s)")
